            margin-bottom: 20px;
        }

        .dashboard-button {
            background-color: rgba(249, 219, 121, 1);
            color: black;
            border: none;
            border-radius: 5px;
            padding: 10px 20px;
            margin-right: 10px;
            cursor: pointer;
            font-size: 14px;
            font-weight: 600;
            transition: background-color 0.3s ease;
        }

        .dashboard-button:hover {
            background-color: rgba(229, 189, 68, 1);
        }

        .dashboard-button:last-child {
            margin-right: 0;
        }
    </style>
</head>

